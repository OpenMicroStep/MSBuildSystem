{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Buildsystem tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": true,
            "args": ["--colors", "test-buildsystem.js" , "-f", "core"],
            "cwd": "${workspaceRoot}/_",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/_/out-buildsystem/node_modules/@msbuildsystem",
            "smartStep": true
        },
        {
            "name": "Bootstrap 5 -> 4",
            "type": "node2",
            "request": "launch",
            "program": "${workspaceRoot}/_/bootstrap5/node env/debug/node_modules/@msbuildsystem/cli/index.js",
            "stopOnEntry": true,
            "args": ["build", "-p", "../@msbuildsystem/", "--color", "-w", "${workspaceRoot}/_/bootstrap4"],
            "cwd": "${workspaceRoot}/_",
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "sourceMaps": true,
            "smartStep": true
        },
        {
            "name": "Bootstrap 3 Aspects",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/_/bootstrap3/node env/debug/node_modules/@msbuildsystem/cli/index.js",
            "stopOnEntry": false,
            "args": ["build", "-p", "/Users/vincentrouille/Dev/MicroStep/Aspects", "--color", "-w", "/tmp/aspects"],
            "cwd": "${workspaceRoot}/_",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/_/bootstrap3/node env/debug/node_modules/@msbuildsystem",
            "smartStep": true
        },
        {
            "name": "Bootstrap Aspects",
            "type": "node",
            "request": "launch",
            "program": "/tmp/bootstrap4/node env/debug/node_modules/@msbuildsystem/cli/index.js",
            "stopOnEntry": false,
            "args": ["build", "-p", "/Users/vincentrouille/Dev/MicroStep/Aspects", "--color", "-w", "/tmp/aspects"],
            "cwd": "/tmp/bootstrap4/node env/debug/node_modules/@msbuildsystem",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "/tmp/bootstrap4/node env/debug/node_modules/@msbuildsystem",
            "smartStep": true
        },
        {
            "name": "Generate",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/_/out-buildsystem/node_modules/@msbuildsystem/cli/src/index.js",
            "stopOnEntry": false,
            "args": ["generate", "-p", "../@msbuildsystem/", "-w", "bootstrap/", "terminal"],
            "cwd": "${workspaceRoot}/_",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/_/out-buildsystem/node_modules/@msbuildsystem",
            "smartStep": true
        },

        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": true,
            "smartStep": true,
            "outDir": "${workspaceRoot}/_/bootstrap2/node env/debug/node_modules/@msbuildsystem",
            "localRoot": "${workspaceRoot}"
        }
    ]
}
