{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Buildsystem tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/_/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            "args": ["--inline-diffs", "--no-timeouts", "--colors", "test-buildsystem.js"],
            "cwd": "${workspaceRoot}/_",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/_/out-buildsystem/node_modules/@msbuildsystem",
            "smartStep": true
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            "restart": false,
            "sourceMaps": true,
            "outDir": null,
            "localRoot": "${workspaceRoot}"
        }
    ]
}