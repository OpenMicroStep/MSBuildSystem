{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "outDir": "../_/out-buildsystem/node_modules/@msbuildsystem",
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "lib": [
      "es6"
    ],
    "types": [
      "node", "mocha", "chai"
    ],
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@msbuildsystem/*": ["./*/src"],
      "@msbuildsystem/core/*": ["./core/*"],
      "@msbuildsystem/shared/*": ["./shared/*"]
    }
  },
  "files" : [
    "cli/src/index.ts",
    "shared/src/index.ts",
    "shared/tst/index.ts",
    "core/src/index.ts",
    "core/tst/index.ts",
    "foundation/src/index.ts",
    "cxx/src/index.ts",
    "cxx/tst/index.ts",
    "cxx.sysroot.darwin/src/index.ts",
    "cxx.tools.clang/install.ts",
    "js/src/index.ts",
    "js/tst/index.ts",
    "js.typescript/src/index.ts",
    "js.typescript/tst/index.ts"
  ]
}
